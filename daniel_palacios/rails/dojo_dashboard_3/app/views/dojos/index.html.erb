<h1>Dojo Dashboard:</h1>

<table>
   <thead>
      <tr>
         <th>Coding Dojo Branch</th>
         <th>Street</th>
         <th>City</th>
         <th>State</th>
         <th>Actions</th>
      </tr>
   </thead>
   <% @dojos.each do |dojo|%>
      <tbody>
         <tr>
            <td><%= dojo.branch %></td>
            <td><%= dojo.street %></td>
            <td><%= dojo.city %></td>
            <td><%= dojo.state %></td>
            <td>
               <a href='/show/<%= dojo.id %>'>Show</a>
               <a href='/edit/<%= dojo.id %>'>Edit</a>
               <form action='/dojos/<%= dojo.id %>' method='post'>
                   <input type="hidden" name="_method" value="delete">
                   <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
                   <input type='submit' value='Delete'>
               </form>
            </td>
         </tr>
      </tbody>
   <% end %>
</table>

<a href='/new'>New Dojo</a>
